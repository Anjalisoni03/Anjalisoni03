-----------------------------------------------30.sum of elements of array-----------------------------
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int arr[]={14,78,11,23,76},sum=0;
    int n=sizeof(arr)/sizeof(arr[0]);
    for(int i=0;i<n;i++)
        sum +=arr[i];
    cout<<sum;
}
-------------------------------------------31. reverse of array---------------------------------
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int arr[]={14,78,11,23,76},sum=0;
    int n=sizeof(arr)/sizeof(arr[0]);
    for(int i=n-1;i>=0;i--)
        cout<<arr[i]<<" ";
}
