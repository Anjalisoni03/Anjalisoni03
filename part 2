---------------------------------------------------------26. largest in array-------------------------
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int arr[]={14,78,11,23,76};
    int n=sizeof(arr)/sizeof(arr[0]);
    int max=INT_MN;
    for(int i=0;i<=n;i++)
    {
        if(arr[i]>max)
            max=arr[i];
    }
    cout<<max;
}
---------------------------------------------------27.smallest array----------------------------
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int arr[]={14,78,11,23,76};
    int n=sizeof(arr)/sizeof(arr[0]);
    int min=INT_MAX;
    for(int i=0;i<n;i++)
    {
        if(arr[i]<min)
            min=arr[i];
    }
    cout<<min;
}
